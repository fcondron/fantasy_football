<%= semantic_form_for @team do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <div class="field">
    <%= f.input :players, :collection => Player.all, :as => :select, :include_blank => "Players" %>
  </div>
  <div class="actions">
    <%= f.submit "Submit" %>
  </div>
<% end %>